[
  {
    "name": "Help",
    "dir": "$FILE_PATH",
    "cmd": "help"
  },
  {
    "name": "C++",
    "dir": "$FILE_PATH",
    "cmd": "g++ $FULL_FILE_NAME -o $FILE_PATH/$FILE_NAME",
    "Rcmd": "bash -c g++ '$FULL_FILE_NAME' -o '$FILE_PATH/$FILE_NAME' && '$FILE_PATH/$FILE_NAME'"
  },
  {
    "name": "Python",
    "dir": "$FILE_PATH",
    "cmd": "python -u $FULL_FILE_NAME",
    "seperator": "[ ]*File \/"
  },
  {
    "name": "Haxe",
    "dir": "$PROJECT_ROOT",
    "cmd": "haxe build.hxml",
    "Rcmd": "start $PROJECT_NAME.n",
    "shortcut": ""
  },
  {
    "name": "Lime Flash",
    "dir": "$PROJECT_ROOT",
    "cmd": "lime build flash",
    "Dcmd": "lime build flash -debug",
    "Rcmd": "start $PROJECT_ROOT/Export/flash/bin/$PROJECT_NAME.swf",
    "shortcut": ""
  }
]